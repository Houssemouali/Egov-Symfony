<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BackOffice\backBundle\Entity\ExtraitNaissance" table="extrait_naissance">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="20" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="20" nullable="false"/>
    <field name="nompere" type="string" column="nomPere" length="20" nullable="false"/>
    <field name="nommere" type="string" column="nomMere" length="20" nullable="false"/>
    <field name="adresseNaissance" type="string" column="adresse_naissance" length="20" nullable="false"/>
    <field name="sex" type="string" column="sex" length="20" nullable="false"/>
    <field name="dateNaissance" type="date" column="date_naissance" nullable="false"/>
    <field name="nationalite" type="string" column="nationalite" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
