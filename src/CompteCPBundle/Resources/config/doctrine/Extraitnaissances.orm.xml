<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CompteCPBundle\Entity\Extraitnaissances" table="extraitnaissances">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="50" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="50" nullable="false"/>
    <field name="sexe" type="string" column="sexe" length="10" nullable="false"/>
    <field name="villeNaissance" type="string" column="ville_naissance" length="50" nullable="false"/>
    <field name="dateNaissance" type="date" column="date_naissance" nullable="false"/>
    <field name="pere" type="string" column="pere" length="50" nullable="true"/>
    <field name="mere" type="string" column="mere" length="50" nullable="true"/>
    <field name="etatCivile" type="string" column="etat_civile" length="50" nullable="false"/>
  </entity>
</doctrine-mapping>
